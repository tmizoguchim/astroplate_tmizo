{
  "site": {
    "title": "Mizoguchi Resarch Group",
    "base_url": "https://astroplate.netlify.app",
    "base_path": "/",
    "trailing_slash": false,
    "favicon": "/images/favicon.png",
    "logo": "/images/NMDLlogo-color.jpg",
    "logo_darkmode": "/images/NMDLlogo-gray.jpg",
    "logo_width": "100",
    "logo_height": "100",
    "logo_text": "NMDL@UTokyo"
  },

  "settings": {
    "search": false,
    "sticky_header": true,
    "theme_switcher": true,
    "default_theme": "system",
    "pagination": 2,
    "summary_length": 300,
    "blog_folder": "blog",
    "default_language": "fr",
    "disable_languages": [],
    "default_language_in_subdir": false
  },

  "params": {
    "contact_form_action": "#",
    "copyright": "Mizoguchi Research Group, <br> Design by [Zeon Studio](https://zeon.studio)"
  },

  "navigation_button": {
    "enable": true,
    "label": "NMDL homepage",
    "link": "https://www.edge.iis.u-tokyo.ac.jp"
  },

  "disqus": {
    "enable": true,
    "shortname": "themefisher-template",
    "settings": {}
  },

  "metadata": {
    "meta_author": "zeon.studio",
    "meta_image": "/images/og-image.png",
    "meta_description": "astro and tailwind boilerplate"
  }
}
